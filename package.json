{
  "name": "Webpack-React-Boilerplate",
  "version": "",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "author": "",
  "license": "MIT",
  "homepage": "",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "build": "better-npm-run clean:build && better-npm-run build",
    "analyze": "better-npm-run clean:build && better-npm-run analyze",
    "lint": "npm-run-all lint:js lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:update-snapshot": "better-npm-run test -u",
    "clean": "npm-run-all clean:build clean:test",
    "clean:build": "better-npm-run clean:build",
    "clean:test": "better-npm-run clean:test",
    "coveralls": "better-npm-run clean:test && better-npm-run coveralls"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start": {
      "command": "node .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/webpack.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "analyze": {
      "command": "webpack -p --progress --hide-modules --config ./tools/webpack/webpack.babel.js",
      "env": {
        "NODE_ENV": "analyze"
      }
    },
    "lint:js": {
      "command": "eslint --fix ."
    },
    "lint:style": {
      "command": "stylelint --fix \"./**/*.css, ./**/*.scss, !coverage\""
    },
    "test": {
      "command": "jest --coverage",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --watchAll",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "clean:build": {
      "command": "rimraf ./public/assets"
    },
    "clean:test": {
      "command": "rimraf ./coverage"
    },
    "coveralls": {
      "command": "cat ./coverage/lcov.info | coveralls"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "jest /src",
      "git add"
    ],
    "*.css": "stylelint",
    "*.{scss,sass}": "stylelint --syntax=scss",
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-loadable/babel",
      "dynamic-import-node",
      "lodash",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console",
          "transform-react-remove-prop-types"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:lodash/recommended",
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "lodash",
      "react",
      "prettier"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack/webpack.babel.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "rules": {
          "no-undef": 0,
          "react/react-in-jsx-scope": 0,
          "react/jsx-no-undef": 0
        }
      }
    ],
    "rules": {
      "no-return-assign": 0,
      "global-require": "off",
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          }
        }
      ],
      "jsx-a11y/label-has-for": [
        "error",
        {
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          }
        }
      ],
      "consistent-return": 0,
      "no-console": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "react/require-default-props": 0,
      "react/destructuring-assignment": 0,
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-cycle": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "to"
          ]
        }
      ],
      "lodash/prefer-lodash-method": "off",
      "lodash/prefer-lodash-typecheck": "off",
      "lodash/import-scope": "off",
      "lodash/prefer-noop": "off"
    }
  },
  "eslintIgnore": [
    "public/assets"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss",
      "stylelint-config-prettier"
    ],
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "string-quotes": "double",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tools/jest/setup.js"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.js",
      "src/pages/**/*.js",
      "src/components/**/*.js",
      "src/actions/**/*.js",
      "src/reducers/**/*.js",
      "!src/actions/index.js",
      "!src/components/index.js",
      "!src/pages/index.js",
      "!src/pages/Home/index.js",
      "!src/pages/UserInfo/index.js",
      "!src/reducers/index.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/jest/assetMock.js"
    },
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "testEnvironment": "jsdom"
  },
  "nodemonConfig": {
    "watch": [
      "api",
      "src/server.js",
      "src/utils/server/renderHtml.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.3",
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^9.4.9",
    "axios": "^0.18.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "better-npm-run": "^0.1.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "connected-react-router": "^6.3.1",
    "consign": "^0.1.6",
    "cross-spawn": "^6.0.5",
    "css-modules-require-hook": "^4.2.3",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "history": "^4.7.2",
    "hpp": "^0.2.2",
    "html-minifier": "^3.5.21",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.16",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dev-utils": "^7.0.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.7.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0-beta.6",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.6.1",
    "serve-favicon": "^2.5.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "check-prop-types": "^1.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "coveralls": "^3.0.3",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^24.1.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-fetch-mock": "^2.1.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.4",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.8.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpackbar": "^3.1.5"
  },
  "resolutions": {
    "webpack-sources": "^1.2.0"
  }
}
